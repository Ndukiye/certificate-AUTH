{
  "version": 2,
  "builds": [
    {
      "src": "api/index.py",
      "use": "@vercel/python"
    }
  ],
  "rewrites": [
    { "source": "/api/(.*)", "destination": "/api/index.py" },
    { "source": "/admin", "destination": "/api/index.py" },
    { "source": "/login", "destination": "/api/index.py" },
    { "source": "/data/(.*)", "destination": "/web/data/$1" },
    { "source": "/(.*)", "destination": "/web/$1" }
  ]
}